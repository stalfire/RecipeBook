<h2>Sign In</h2>

	 <%= form_for(:session, url: login_path) do |f| %>

      <%= f.label :email %>
      <%= f.email_field :email%>

      <%= f.label :password %>
      <%= f.password_field :password%>

      <%= f.submit "Log in"%>
    <% end %>

<%= link_to t(".sign_up"), sign_up_path %>

<%= link_to "Sign up with Facebook", "/auth/facebook" %>
